{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "cy:open": "cypress open",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e --browser chrome",
    "cy:run-unit": "cypress run --component --browser chrome",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:open-e2e",
    "vitest": "vitest run",
    "vitest:changed": "vitest run --changed",
    "vitest:staged": "vitest related --run --passWithNoTests --environment=jsdom",
    "vitest:coverage": "vitest run --coverage",
    "vitest:watch": "vitest watch",
    "storybook": "start-storybook -p 6006",
    "test-storybook": "test-storybook",
    "test-storybook:update": "test-storybook -u",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\"",
    "build-storybook": "build-storybook",
    "test": "yarn test-storybook:ci && yarn vitest && yarn cy:run-unit",
    "coverage": "rm -rf coverage && yarn test-storybook:ci && yarn vitest:coverage && yarn cy:run-unit && yarn coverage:merge && yarn coverage:merge-report",
    "coverage:merge": "mkdir coverage/__temp__ && cp ./coverage/unit/coverage-final.json coverage/__temp__/unit.json && cp ./coverage/cypress/coverage-final.json coverage/__temp__/cypress.json && nyc merge coverage/__temp__ coverage/merged/coverage-final.json && rm -rf coverage/__temp__",
    "coverage:merge-report": "nyc report --reporter=lcov --reporter=text --temp-dir=./coverage/merged --report-dir=./coverage/merged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn vitest:staged"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@cypress/code-coverage": "^3.10.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@joengsh/eslint-config-react": "^1.1.2",
    "@joengsh/prettier-config": "^1.0.3",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^6.5.14",
    "@storybook/test-runner": "^0.9.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@swc/core": "^1.3.23",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-istanbul": "^0.26.2",
    "@vitest/ui": "^0.26.2",
    "babel-loader": "^8.3.0",
    "camelcase": "6",
    "concurrently": "^7.6.0",
    "cypress": "^12.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.6.8",
    "http-server": "^14.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-image-snapshot": "^6.1.0",
    "jest-watch-typeahead": "^2.2.1",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.5.1",
    "tw-elements": "^1.0.0-alpha13",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-aliases": "0.9.7",
    "vite-plugin-checker": "^0.5.2",
    "vite-plugin-istanbul": "^3.0.4",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.25.8",
    "wait-on": "^7.0.1"
  },
  "resolutions": {
    "istanbul-lib-coverage": "^3.0.0",
    "istanbul-lib-hook": "^3.0.0",
    "istanbul-lib-instrument": "^4.0.0",
    "istanbul-lib-processinfo": "^2.0.2",
    "istanbul-lib-report": "^3.0.0",
    "istanbul-lib-source-maps": "^4.0.0",
    "istanbul-reports": "^3.0.2",
    "nyc": "~15.0.0"
  }
}
